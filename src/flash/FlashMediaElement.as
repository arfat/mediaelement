gth = cr.length;				if(cr_length > 1)				{					for (var cnt=0;cnt < cr_length;cnt++)					{						if(cr[cnt].sequence == 1)						{							_selectedCreative = cr[cnt].source;							setVideo(cr[cnt].source.mediaFiles[0].uri,"pre");							break;						}					}				}								if(_selectedCreative.videoClicks)				{					if(_selectedCreative.videoClicks.clickThrough)					{						_clickFlgForBgMcForVideoClickEvents = true;					}					var clickTracking = _selectedCreative.videoClicks.clickTracking;					if(clickTracking)					{						_clickFlgForBgMcForVideoClickEvents = true;					}										var customClicks = _selectedCreative.videoClicks.customClicks;					if(customClicks)					{						_clickFlgForBgMcForVideoClickEvents = true;					}				}				videoprerollFlg = true;			}else{				setDefaultVideo();			}					}		//used for viewing playbtn on videofullarea		public function showCenterPlayBtn(){			(_clickFlgForBgMcForVideoClickEvents)?_playFullBtnMc.playbtn.visible = true: ((_showControls)?_playFullBtnMc.visible = true:_playFullBtnMc.visible = false);		}		public function setDefaultVideo()		{						stopMedia();			ExternalInterface.call("mejs.players."+_flashid.split("_flash").join("p")+".pause()");			try			{				removeChild(_skipAdMc);			}catch(e){};			removeChild(_video);			if(!_autoplay)			{				(_clickFlgForBgMcForVideoClickEvents)?_playFullBtnMc.playbtn.visible = true: ((_showControls)?_playFullBtnMc.visible = true:_playFullBtnMc.visible = false);			}			_playButton.visible = true;			_pauseButton.visible = false;			setVideo(_mediaUrl,"content");					}									private function SkipTimerFunc(e:TimerEvent)		{			if(_skipAdTimerCount !=0)			{				_skipAdTimerCount--;				setSkipTxt();			}else{				_skipAdMc.gotoAndStop(3);				_skipTimer.stop();				_skipAdMc.buttonMode = true;				_skipAdMc.addEventListener(MouseEvent.CLICK,onSkipAd);			}		}				private function setSkipTxt ()		{			_skipAdMc.gotoAndStop(2);			var txtfmt:TextFormat= new TextFormat();			txtfmt.size = 11;			txtfmt.letterSpacing = 2;						_skipAdMc.skipcnttxt.multiline=true;			_skipAdMc.skipcnttxt.defaultTextFormat = txtfmt;				_skipAdMc.skipcnttxt.setTextFormat(_skipAdMc.skipcnttxt.defaultTextFormat);				_skipAdMc.skipcnttxt.text = "YOU CAN SKIP THIS AD IN "+(_skipAdTimerCount+1);		}				private function onSkipAd(e:MouseEvent)		{			_mediaElement.iabEventsinVideo(TrackingEventTypes.ADSKIPPED);			setDefaultVideo();		}				public function setPoster(){			if(_posterSrc) {				_output.appendText("poster: " + _posterSrc + "\n");				var _posterSrcErr = _posterSrc;				var _posterSrcCall = _posterSrc;								if(_posterSrc.indexOf("ytimg") != -1)				{					_posterSrcCall = (_posterSrc.substr(0,_posterSrc.lastIndexOf("/")) + "/maxresdefault.jpg");				}								_posterObj = new Loader();				var context:LoaderContext = new LoaderContext(); 				context.checkPolicyFile = true; 								_posterObj.load(new URLRequest(_posterSrcCall),context);								var _stht =  stage.stageHeight -_controlBarBg.height+1;								_posterObj.contentLoaderInfo.addEventListener(IOErrorEvent.IO_ERROR, function fn(e):void {					if(_posterSrcErr.indexOf("ytimg") != -1)					{						_posterSrcCall = (_posterSrcErr.substr(0,_posterSrcErr.lastIndexOf("/")) + "/mqdefault.jpg");						_posterObj.load(new URLRequest(_posterSrcCall),context);					}															  				});				_posterObj.contentLoaderInfo.addEventListener(Event.COMPLETE, function fn(e:Event):void {					var bit:Bitmap = e.target.content;					if(bit != null)					bit.smoothing = true;										var stageRatio:Number = stage.stageWidth / _stht ;					var loaderRatio:Number = _posterObj.width / _posterObj.height;					if(stageRatio < loaderRatio){						_posterObj.width = stage.stageWidth;						_posterObj.scaleY = _posterObj.scaleX;					}else{						_posterObj.height = _stht;						_posterObj.scaleX = _posterObj.scaleY;					}					_posterObj.x = (stage.stageWidth - _posterObj.width)/2;					_posterObj.y = (_stht - _posterObj.height)/2;					if(this.removeEventListener) this.removeEventListener(Event.COMPLETE, fn);				});				addChild(_posterObj);			}						setControlDepth();		}		private function removePoster () {			if(_posterObj && _posterObj.stage) {				_posterObj.parent.removeChild(_posterObj);				//_posterObj.visible = false;			}		}	}}