idth =  _scrubOverlay.width = _scrubTrack.width =  (_duration.x-_duration.width-10)-_duration.width-10;								_scrubMc.x = _scrubLoaded.x-(_scrubMc.width/2);				_scrubMc.y = Math.round(_scrubBar.y - (_scrubMc.height-_scrubBar.height)/2) + 2;				_controlBar.x = 0;				_controlBar.y = stage.stageHeight - _controlBarBg.height +3;								_skipAdMc.x = stage.stageWidth - _skipAdMc.width;				_skipAdMc.y =_controlBar.y - _skipAdMc.height -  10;								_playFullBtnMc.playbtn.playbtninnerbgMc.width = _playFullBtnMc.playbtnbgMc.width = stage.stageWidth;				_playFullBtnMc.playbtn.playbtninnerbgMc.height = _playFullBtnMc.playbtnbgMc.height = stage.stageHeight-_controlBarBg.height +3;				_playFullBtnMc.playbtn.playbtninnerbgMc.alpha = 0;				_playFullBtnMc.x =_playFullBtnMc.playbtnbgMc.x = 0;				_playFullBtnMc.y =_playFullBtnMc.playbtnbgMc.y = 0;				_playFullBtnMc.playbtn.playbtninnerbgMc.x = - _playFullBtnMc.playbtn.playbtninnerbgMc.width/2;				_playFullBtnMc.playbtn.playbtninnerbgMc.y = - _playFullBtnMc.playbtn.playbtninnerbgMc.height/2;				_playFullBtnMc.playbtn.x = _playFullBtnMc.playbtn.width/2;				_playFullBtnMc.playbtn.y =_playFullBtnMc.playbtn.height/2;								//setControlDepth();				_fullscreenButton.alpha = 1;				_controlBar.alpha = 1;				_skipAdMc.alpha = 1;				_playFullBtnMc.alpha = 1;		}		// END: Controls				public function playbtnClick(e:MouseEvent = null):void{			if(_isEnded) {				_mediaElement.setCurrentTime(0);			}			_mediaElement.play();			removePoster();		}				public function playFullBtnMcClick(e:MouseEvent = null):void{			if(_mediaElement.getPaused() || _isEnded)			{				playbtnClick()			}else{				_mediaElement.pause();			}					}						public function onbtnMouseOut(e:MouseEvent):void{			if(e.currentTarget.name == "playFullBtnMc"){				e.currentTarget.playbtn.gotoAndStop(1);			}			else if(e.currentTarget.currentFrame == 4)			{				e.currentTarget.gotoAndStop(3);			}else{				e.currentTarget.gotoAndStop(1);			}